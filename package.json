{
  "name": "weio",
  "version": "0.1.2",
  "description": "Promise based HTTP client for for wechat miniprogram",
  "main": "./index.js",
  "module": "./lib/weio.esm.mjs",
  "types": "./lib/weio.d.ts",
  "typings": "./lib/weio.d.ts",
  "repository": "https://github.com/yoneyy/weio.git",
  "author": {
    "name": "yoneyy",
    "email": "yoneyy@163.com",
